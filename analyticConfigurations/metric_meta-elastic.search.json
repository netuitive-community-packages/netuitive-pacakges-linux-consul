{
  "analyticConfiguration": {
     "metrics": [
      {
        "match": "consul.catalog.nodes_(critical|passing|up|warning|nodes|critical|passing|up|warning)$",
        "properties": {
          "tags": {
            "unit": "count"
          }
	}
      },
      {
        "match": "consul.catalog.services_(critical|passing|up|warning|nodes|critical|passing|up|warning)$",
        "properties": {
          "tags": {
            "unit": "count"
          }
        }
      },
      {
	"match": "consul.runtime.*",
	"properties": {
	   "statistic": "MAX",
	   "type": "COUNTER"
	  }
      },
      {
	"match": "consul.counter.*",
	"properties": {
	  "statistic": "MAX",
	  "type": "COUNTER"
	  }
      },
      {
	"match": "consul.raft.*",
	"properties": {
	  "statistic": "MAX",
	  "type": "COUNTER"
	  }
      },
      {
	"match": "consul.serf.*",
	"properties": "MAX",
	  "statistic": "MAX",
	  "type": "COUNTER"
	  }
      }
     ],
	"name": "Consul",
	"scope": {
		"elementType": "SERVER",
		"metricMatches": "^consul\\..*"
	},
	"type": "METRIC_META"
  }
}

